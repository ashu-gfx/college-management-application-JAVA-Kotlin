    android:layout_width="match_parent"
    android:id="@+id/relative_layout1"
    android:layout_height="match_parent"
    tools:context=".menupage2">

    <ScrollView
        android:layout_width="match_parent"

        android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:background="@drawable/ic_menupage_background"
        android:orientation="vertical">

        <TextView
            android:id="@+id/hello"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="31dp"
            android:layout_marginTop="74dp"
            android:fontFamily="@font/barlow_semiboldd"
            android:text="Hello,"
            android:textColor="#0B121F"
            android:textSize="28sp" />

        <TextView
            android:id="@+id/username11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="74dp"
            android:layout_toEndOf="@+id/hello"
            android:fontFamily="@font/barlow_semiboldd"
            android:inputType="textCapSentences"
            android:text="User Name            "
            android:textColor="#0B121F"
            android:textSize="28sp"
            tools:ignore="TextViewEdits" />

        <TextView
            android:id="@+id/discription"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="31dp"
            android:layout_marginTop="102dp"
            android:layout_marginEnd="164dp"
            android:fontFamily="@font/barlow_regular"
            android:text="welcome back !"
            android:textColor="#70747E"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="183dp"
            android:layout_marginRight="-2dp"
            app:srcCompat="@drawable/ic_enrollment_display" />

        <ImageButton
            android:id="@+id/m1_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="368dp"
            android:layout_marginEnd="197dp"
